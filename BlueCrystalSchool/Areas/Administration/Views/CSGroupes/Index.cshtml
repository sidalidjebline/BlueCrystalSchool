@model IEnumerable<BlueCrystalSchool.Areas.CoursSoutiens.Models.CSGroupe>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CSEnseignant.Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CSMatiere.NomMatiere)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CsNiveauScolaire.Niveau)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomGroupe)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CSEnseignant.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CSMatiere.NomMatiere)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CsNiveauScolaire.Niveau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomGroupe)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
